<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <!--Generiek element voor het specificeren van een geselecteerde HB locatie.
  In dit element moet de naam van de hydraulische belastingenlocatie
  zoals ook zichtbaar in Riskeer worden opgenomen.-->
  <xs:element name="hblocatie">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="1"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
</xs:schema>