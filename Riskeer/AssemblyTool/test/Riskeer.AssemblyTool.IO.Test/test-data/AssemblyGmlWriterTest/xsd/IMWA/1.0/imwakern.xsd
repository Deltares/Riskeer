  <element name="Kunstwerk" type="imwa:KunstwerkType">
    <annotation>
      <documentation>Civieltechnisch werk voor de infrastructuur van wegen, water, spoorbanen, waterkeringen en/of leidingen niet bedoeld voor permanent menselijk verblijf.</documentation>
    </annotation>
  </element>
  <complexType name="KunstwerkType">
    <complexContent>
      <extension base="gml:AbstractFeatureType">
        <sequence>
          <element maxOccurs="unbounded" minOccurs="0" name="geometrie2D">
            <annotation>
              <documentation>Geometrische representatie van het object als punt-, lijn-, of (multi)vlak.</documentation>
            </annotation>
            <complexType>
              <choice minOccurs="0"/>
              <attributeGroup ref="gml:AssociationAttributeGroup"/>
            </complexType>
          </element>
          <element minOccurs="0" name="indicatieWaterkerend" type="gml:CodeType">
            <annotation>
              <documentation>Indicatie voor het onderscheid tussen waterkerende en niet-waterkerende kunstwerken.</documentation>
            </annotation>
          </element>
          <element minOccurs="0" name="typeKunstwerk" type="gml:CodeType">
            <annotation>
              <documentation>Nadere aanduiding van het type kunstwerk. Dit is een 'Conditioneel' attribuut, omdat het kunstwerkdeel in afzonderlijke klassen is gespecialiseerd.</documentation>
            </annotation>
          </element>
          <element minOccurs="0" name="typeWaterkerendeConstructie" type="gml:CodeType">
            <annotation>
              <documentation>WaterkerendeConstructieType: Typering van een constructie in relatie tot de waterkerende functie (volgens de leidraad waterkerende kunstwerken).</documentation>
            </annotation>
          </element>
          <element minOccurs="0" name="isOnderdeelVan"/>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
