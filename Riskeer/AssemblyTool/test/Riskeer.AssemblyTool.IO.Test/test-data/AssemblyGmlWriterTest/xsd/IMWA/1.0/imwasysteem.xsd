          </element>
          <element minOccurs="0" name="typeWaterKwantitatief" type="gml:CodeType">
            <annotation>
              <documentation>WaterTypeKwantitatief: Een aanduiding voor het soort oppervlaktewater beschouwd vanuit het oogpunt van waterkwantiteit.</documentation>
            </annotation>
          </element>
          <element maxOccurs="unbounded" name="bevatWaterdeel" type="imgeo:WaterdeelPropertyType"/>
          <element maxOccurs="unbounded" name="bevatOndersteunendWaterdeel" type="imgeo:OndersteunendWaterdeelPropertyType"/>
          <element maxOccurs="unbounded" name="geschematiseerdDoor" type="OppervlaktewaterSegmentPropertyType"/>
