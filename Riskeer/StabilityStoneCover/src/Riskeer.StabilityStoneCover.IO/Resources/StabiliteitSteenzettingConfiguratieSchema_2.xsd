<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <!--Basis elementen voor een bekledingen configuratie.-->
  <xs:include schemaLocation="BekledingenConfiguratieBasisSchema_2.xsd" />
  <xs:include schemaLocation="ConfiguratieSchema.xsd" />

  <!--Herdefinieer categoriegrens, zodat deze de juiste inhoud heeft.-->
  <xs:redefine schemaLocation="BekledingenConfiguratieBasisSchema.xsd">
    <xs:simpleType name="bekledingType">
      <xs:restriction base="bekledingType">
        <xs:enumeration value="Steen (blokken)"/>
        <xs:enumeration value="Steen (zuilen)"/>
        <xs:enumeration value="Steen (blokken en zuilen)"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:redefine>
</xs:schema>