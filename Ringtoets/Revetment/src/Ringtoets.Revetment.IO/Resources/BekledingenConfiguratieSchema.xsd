<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <!--Basis elementen voor een bekledingen configuratie.-->
  <xs:include schemaLocation="BekledingenConfiguratieBasisSchema.xsd" />
  <xs:include schemaLocation="ConfiguratieSchema.xsd" />

  <!--Herdefinieer categorie grens, zodat deze de juiste inhoud heeft.-->
  <xs:redefine schemaLocation="BekledingenConfiguratieBaisSchema.xsd">
    <xs:simpleType name="categoriegrensType">
      <xs:restriction base="categoriegrensType">
        <xs:enumeration value="A+-A"/>
        <xs:enumeration value="A-B"/>
        <xs:enumeration value="B-C"/>
        <xs:enumeration value="C-D"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:redefine>
</xs:schema>