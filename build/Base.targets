<Project>
  <PropertyGroup>
    <OutputPath Condition="$(OutputPath) == ''">bin\$(Configuration)\</OutputPath>
    <OutDir>$(OutputPath)</OutDir>
    <Platforms>x86</Platforms>
    <CodeAnalysisRuleSet>MinimumRecommendedRules.ruleset</CodeAnalysisRuleSet>
    <TargetFramework>net7.0-windows</TargetFramework>
    <LangVersion>7.0</LangVersion>
    <FileAlignment>512</FileAlignment>
    <Company>Deltares</Company>
    <Copyright>Copyright Â© Deltares 2022</Copyright>
    <Version>22.2.1</Version>
    <Configurations>Debug;Release;Coverage</Configurations>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Debug|x86'">
    <ConfigurationGroup>Debug</ConfigurationGroup>
    <DebugSymbols>true</DebugSymbols>
    <DebugType>full</DebugType>
    <Optimize>false</Optimize>
    <DefineConstants>TRACE;DEBUG</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Release|x86'">
    <ConfigurationGroup>Release</ConfigurationGroup>
    <DebugType>none</DebugType>
    <Optimize>true</Optimize>
    <DefineConstants>TRACE;RELEASE</DefineConstants>
    <AllowedReferenceRelatedFileExtensions>none</AllowedReferenceRelatedFileExtensions>
    <IncludeSourceRevisionInInformationalVersion>false</IncludeSourceRevisionInInformationalVersion>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Coverage|x86'">
    <ConfigurationGroup>Release</ConfigurationGroup>
    <DefineConstants>TRACE;COVERAGE</DefineConstants>
    <Optimize>true</Optimize>
    <DebugType>pdbonly</DebugType>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="MSBuildConfigurationDefaults" Version="1.0.1" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="$(SolutionDir)Core\Common\src\Core.Common.Version\Properties\GlobalAssembly.cs">
      <Link>Properties\GlobalAssembly.cs</Link>
    </Compile>
  </ItemGroup>
</Project>